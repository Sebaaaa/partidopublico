<p id="notice"><%= notice %></p>

<h1>Listing Circunscripcions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @circunscripcions.each do |circunscripcion| %>
      <tr>
        <td><%= link_to 'Show', circunscripcion %></td>
        <td><%= link_to 'Edit', edit_circunscripcion_path(circunscripcion) %></td>
        <td><%= link_to 'Destroy', circunscripcion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Circunscripcion', new_circunscripcion_path %>
