<%= simple_form_for(@acuerdo) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :numero %>
    <%= f.input :fecha %>
    <%= f.input :tipo %>
    <%= f.input :tema %>
    <%= f.input :region %>
    <%= f.association :organo_interno %>
    <%= f.input :documento %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
