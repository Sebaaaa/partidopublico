<%= simple_form_for(@ley) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :numero %>
    <%= f.input :nombre %>
    <%= f.input :enlace %>
    <%= f.input :tags %>
    <%= f.input :resumen %>
    <%= f.association :marco_general %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
