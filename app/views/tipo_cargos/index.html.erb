<p id="notice"><%= notice %></p>

<h1>Listing Tipo Cargos</h1>

<table>
  <thead>
    <tr>
      <th>Titulo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_cargos.each do |tipo_cargo| %>
      <tr>
        <td><%= tipo_cargo.titulo %></td>
        <td><%= link_to 'Show', tipo_cargo %></td>
        <td><%= link_to 'Edit', edit_tipo_cargo_path(tipo_cargo) %></td>
        <td><%= link_to 'Destroy', tipo_cargo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo cargo', new_tipo_cargo_path %>
